  "description": "Framework WebDjangular 1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve client --port=4200 --host 0.0.0.0 --proxy-config proxy.conf.json",
    "start:admin": "ng serve admin --port=4201 --proxy-config proxy.conf.json",
    "start:prerender": "ng serve prerender",
    "build": "ng build client",
    "build:admin": "ng build admin",
    "build:prod": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng build client --prod",
    "build:admin:prod": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng build admin --prod",
    "test": "ng test",
    "lint": "./node_modules/.bin/nx lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "./node_modules/.bin/nx affected:apps",
    "affected:libs": "./node_modules/.bin/nx affected:libs",
    "affected:build": "./node_modules/.bin/nx affected:build",
    "affected:e2e": "./node_modules/.bin/nx affected:e2e",
    "affected:test": "./node_modules/.bin/nx affected:test",
    "affected:lint": "./node_modules/.bin/nx affected:lint",
    "affected:dep-graph": "./node_modules/.bin/nx affected:dep-graph",
    "format": "./node_modules/.bin/nx format:write",
    "format:write": "./node_modules/.bin/nx format:write",
    "format:check": "./node_modules/.bin/nx format:check",
    "update": "ng update @nrwl/schematics",
    "update:check": "ng update",
    "workspace-schematic": "./node_modules/.bin/nx workspace-schematic",
    "dep-graph": "./node_modules/.bin/nx dep-graph",
    "help": "./node_modules/.bin/nx help",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "node dist/server",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:ssr:prod": "npm run build:client-and-server-bundles:production && npm run compile:server",
    "build:client-and-server-bundles:prod": "ng build --prod && ng run client:server:production",
    "build-all:prod": "npm run build:prod && npm run build:admin:prod && ng run client:server:production && npm run compile:server",
    "build-and-serve:prod": "npm run build-all:prod && npm run serve:ssr",
    "build-all": "ng build && ng build:admin && ng run client:server && npm run compile:server",
    "build-and-serve": "npm run build-all && npm run serve:ssr",
    "ng-high-memory": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MyOwnGamesLLC/WebDjangular.git"
